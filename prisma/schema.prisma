generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  username String @unique
  password String
  cardid String @unique @db.VarChar(10)
  mobile String @unique @db.VarChar(13)
  status Boolean 
}
